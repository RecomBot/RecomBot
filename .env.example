# .env - ОБНОВЛЕННЫЙ ФАЙЛ (НИКОМУ НЕ ПОКАЗЫВАТЬ!)

# Telegram Bot (НОВЫЙ ТОКЕН)
BOT_TOKEN=ВАШ_НОВЫЙ_TELEGRAM_BOT_TOKEN_ЗДЕСЬ

# База данных (PostgreSQL)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/travel_db

# LLM (Ollama Cloud) (НОВЫЙ КЛЮЧ)
OLLAMA_BASE_URL=https://ollama.com
OLLAMA_MODEL=qwen3-coder:480b-cloud
OLLAMA_API_KEY=ВАШ_НОВЫЙ_OLLAMA_API_KEY_ЗДЕСЬ

# Redis
REDIS_URL=redis://localhost:6379/0

# URLs
API_BASE_URL=http://localhost:8000/api/v1

# Настройки парсера
PARSE_CITY=Moscow
MAX_PAGES_PER_CATEGORY=3
REQUEST_DELAY=2.0
LOG_LEVEL=INFO

# Настройки разработки
DEBUG=true
RELOAD=true